{
  "name": "nuxt-with-express",
  "version": "1.0.0",
  "description": "Running NUXT in parallel with Express. Blog post available here, https://medium.com/@johnryancottam/running-nuxt-in-parallel-with-express-ffbd1feef83c",
  "author": "John Ryan Cottam",
  "private": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "nuxt": "^2.18.1",
    "cross-env": "^7.0.3",
    "express": "^4.21.2",
    "@nuxtjs/axios": "^5.13.6"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "@nuxtjs/eslint-config": "^2.0.2",
    "@nuxtjs/eslint-module": "^1.2.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-nuxt": "^0.5.2",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "prettier": "^2.8.8"
  }
}
